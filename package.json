{
  "name": "react-music-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://kb-pv-05.github.io/REACT-MUSIC-APP/",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start:db": "json-server --watch mock-api/db.json --port 3001",
    "start:songs": "json-server --watch mock-api/songs.json --port 3002",
    "start:all": "concurrently \"npm run start:db\" \"npm run start:songs\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "json-server": "^1.0.0-beta.3",
    "vite": "^6.2.0"
  }
}
